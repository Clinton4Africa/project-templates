{
    "manifest_version": 2,
    "name": "Hello World",
    "short_name": "Hello World",
    "description": "Hello World App",
    "version": "1.0.0",
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "*://*.monaca.mobi/*",
        "clipboardWrite",
        "clipboardRead",
        "unlimitedStorage",
        "storage"
    ],
    "offline_enabled": false,
    "optional_permissions": null,
    "externally_connectable": {
        "ids": [
            ""
        ],
        "matches": [
            ""
        ],
        "accepts_tls_channel_id": false
    }
}